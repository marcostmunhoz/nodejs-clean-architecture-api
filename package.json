{
  "name": "typescript-nodejs-course",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:watch": "jest --watch --silent --runInBand",
    "test:unit": "npm test -- --config jest-unit.config.ts",
    "test:integration": "npm test -- --config jest-integration.config.ts",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --silent --noStackTrace --coverage",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@shelf/jest-mongodb": "^2.1.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.0.1",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.9.1",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "lint-staged": "^11.1.2",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint 'src/**' --fix",
      "npm run test:staged"
    ]
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "mongodb": "^4.1.2",
    "validator": "^13.6.0"
  },
  "engines": {
    "node": "^16"
  }
}
